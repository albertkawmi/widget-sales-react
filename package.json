{
  "name": "widget-sales-react",
  "version": "0.1.0",
  "description": "A simple React web UI for widget-sales-api",
  "private": true,
  "dependencies": {
    "dotenv": "^4.0.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-router-dom": "4.2.2",
    "react-scripts": "1.0.17"
  },
  "scripts": {
    "start": "react-scripts start",
    "now-start": "cd build && serve -s ./",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:snapshots": "jest --updateSnapshot",
    "test:coverage": "npm test -- --coverage",
    "eslint": "eslint ./src",
    "eject": "react-scripts eject",
    "now": "now alias $(now --public) widget-sales-react",
    "deploy": "npm run eslint && npm run test:coverage && npm run build && npm run now"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/albertkawmi/widget-sales-react.git"
  },
  "keywords": [
    "react",
    "example"
  ],
  "author": "Albert Kawmi <albert@kawmi.co> (http://kawmi.co/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/albertkawmi/widget-sales-react/issues"
  },
  "homepage": ".",
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "now": "^9.1.0",
    "serve": "^6.4.8"
  }
}
